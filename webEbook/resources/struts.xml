<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="ebook" extends="struts-default">
		<action name="home" class="Action.Struts2.PostAction" method="execute">
			<result name="success">/home.jsp</result>
		</action>
		
		<action name="detailPost" class="Action.Struts2.PostAction" method="detailPost">
			<result name="success">/home.jsp</result>
		</action>
		<action name="loginAction" class="Action.Struts2.UserAction" method="login">
			<result name="success">/test.jsp</result>
			<result name="fail">/error.jsp</result>
		</action>
		<action name="logoutAction" class="Action.Struts2.UserAction" method="logout">
			<result name="success">/test.jsp</result>
		</action>
		<action name="registerAction" class="Action.Struts2.UserAction" method="register">
			<result name="success">/test.jsp</result>
			<result name="fail">/error.jsp</result>
		</action>
		
	</package>
</struts>